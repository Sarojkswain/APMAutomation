            <stringProp name="BeanShellSampler.query">import java.util.*;
import java.text.SimpleDateFormat;
import java.util.concurrent.*;

cal = props.get(&quot;startTime&quot;);

String getRandomAttributes() {
    r = ThreadLocalRandom.current();

    et = &quot;&amp;eventType=&quot;;
    SC = &quot;STATUS_CHANGE&quot;;
    TC = &quot;TOPOLOGICAL_CHANGE&quot;;
    GAC = &quot;GATHERED_ATTRIBUTES_CHANGE&quot;;

    randNumber = r.nextInt(7);

    switch (randNumber) {
        case 0: return et + SC;
        case 1: return et + TC;
        case 2: return et + GAC;
        case 3: return et + SC + et + TC;
        case 4: return et + SC + et + GAC;
        case 5: return et + TC + et + GAC;
        case 6: return et + SC + et + TC + et + GAC;
        default: return &quot;&quot;;
    }
}

String getTime(Calendar cal) {
    dFormat = new SimpleDateFormat(&quot;YYYY-MM-dd&quot;);
    tFormat = new SimpleDateFormat(&quot;HH:mm:ss.SSS&quot;);


    return dFormat.format(cal.getTime()) + &quot;T&quot; + tFormat.format(cal.getTime()) + &quot;Z&quot;;
}

String getDatesFormatted(Calendar cal, int CalendarItemToChange, int changeValue) {
    S = &quot;&amp;&quot;;
    StringBuilder sb = new StringBuilder();

    String viewPortStartTime = getTime(cal);
    cal.add(Calendar.MINUTE, 1);
    String startTime = getTime(cal);
    cal.add(CalendarItemToChange, changeValue);
    String endTime = getTime(cal);
    cal.add(Calendar.MINUTE, 1);
    String viewPortEndTime = getTime(cal);


    sb.append(&quot;?viewPortStart=&quot;).append(viewPortStartTime)
        .append(S).append(&quot;startTime=&quot;).append(startTime)
        .append(S).append(&quot;endTime=&quot;).append(endTime)
        .append(S).append(&quot;viewPortEnd=&quot;).append(viewPortEndTime);
    return sb.toString();
}

vars.put(&quot;hours&quot;, getDatesFormatted(cal, Calendar.HOUR, 1));
vars.put(&quot;eventType&quot;, getRandomAttributes());</stringProp>
