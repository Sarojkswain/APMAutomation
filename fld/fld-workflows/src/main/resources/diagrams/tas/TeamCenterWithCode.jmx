              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.regex.Matcher;
import java.util.regex.Pattern;
import java.util.*;
import java.util.concurrent.*;

response = prev.getResponseDataAsString();

Integer[] getRandomNumbers(int maxCount, int maxValue) {
    rand = ThreadLocalRandom.current();
    count = rand.nextInt(maxCount);

    nums = new Integer[count];

    for (int z = 0; z &lt; count; z++) {
        nums[z] = rand.nextInt(maxValue);
    }

    return nums;
}

ArrayList getVertexesList() {
    vertexes = new ArrayList();

    pattern = &quot;vertexId\&quot;:\&quot;([a-zA-Z0-9.:]+)\&quot;&quot;;
    regex = Pattern.compile(pattern);

    m = regex.matcher(response);

    while (m.find()) {
        try {
            for (int z = 0; z &lt; m.groupCount(); z++) { vertexes.add(m.group(z)); }
        } catch (Exception ex) {}
    }
    return vertexes;
}

String getVertexForRequest(ArrayList vertexes, int maxCount)
{
    if (maxCount&gt;vertexes.size())
    { maxCount = vertexes.size(); }

    randoms = getRandomNumbers(maxCount, vertexes.size());

    sb = new StringBuilder();
    sb.append(&quot;{\&quot;vertexIds\&quot;:[&quot;);

    for (int z=0; z&lt;randoms.length; z++)
    {
        sb.append(&quot;\&quot;&quot;);
        sb.append(vertexes.get(randoms[z]).substring(&quot;vertexId\&quot;:\&quot;&quot;.length()));
        if (z != randoms.length - 1) { sb.append(&quot;,&quot;); }
    }

    sb.append(&quot;],\&quot;timestamp\&quot;:null}&quot;);
    return sb.toString();
}

maxCount = 100;
vertexes = getVertexesList();
res = getVertexForRequest(vertexes, maxCount);

vars.put(&quot;vertex&quot;, res);
log.info(&quot;Response size : &quot; + response.length());
log.info(&quot;vertexes size: &quot; + res.length());</stringProp>
