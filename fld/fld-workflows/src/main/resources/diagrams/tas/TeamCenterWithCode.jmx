          <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Generate Perspective" enabled="true">
            <stringProp name="BeanShellSampler.query">import java.util.*;
import java.util.concurrent.*;

possibilities = &quot;Application,backendName,Business Service,connectionProvider,deviceOS,deviceOSVersion,domain,endUser,Hostname,Location,mom,Name,owner,servletClassname,servletMethod,socketHostname,socketPort,tier,transactionId,transactionName,transactionType,Type,wsNamespace,wsOperation&quot;;
possibilities = possibilities.split(&quot;,&quot;);

Integer[] getRandomNumbers(int maxCount, int maxValue) {

    r = ThreadLocalRandom.current();
    count = 1+r.nextInt(maxCount);

    nums = new Integer[count];

    for (int z = 0; z &lt; count; z++) {
        nums[z] = r.nextInt(maxValue);
    }

    return nums;
}

r = ThreadLocalRandom.current();

numberOfRandoms = 9;
randoms = getRandomNumbers(numberOfRandoms, possibilities.length);

sb = new StringBuilder();
name = &quot;JMETER-VIEW-&quot; + r.nextInt();

sb.append(&quot;{\&quot;id\&quot;:\&quot;0\&quot;,\&quot;name\&quot;:\&quot;&quot;);
sb.append(name);
sb.append(&quot;\&quot;,\&quot;groupBy\&quot;:[&quot;);
for (int z = 0; z &lt; randoms.length - 1; z++) {
    sb.append(&quot;{\&quot;attributeName\&quot;:\&quot;&quot;).append(possibilities[randoms[z]]).append(&quot;\&quot;,\&quot;prefix\&quot;:\&quot;\&quot;}&quot;);
    sb.append(&quot;,&quot;);
}
sb.append(&quot;{\&quot;attributeName\&quot;:\&quot;&quot;).append(possibilities[randoms[randoms.length - 1]]).append(&quot;\&quot;,\&quot;prefix\&quot;:\&quot;\&quot;}&quot;);

sb.append(&quot;],\&quot;public\&quot;:false,\&quot;order\&quot;:14,\&quot;owner\&quot;:\&quot;admin\&quot;}&quot;);
sb.append(&quot;\r\n&quot;);

vars.put(&quot;perspectiveName&quot;, name);
vars.put(&quot;perspective&quot;, sb.toString());</stringProp>
