	<build>
		<finalName>LoadOrchestrator</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-enforcer-plugin</artifactId>
			</plugin>

			<plugin>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>copy</id>
						<goals>
							<goal>unpack</goal>
						</goals>
						<phase>generate-resources</phase>
						<configuration>
							<outputDirectory>${project.build.directory}/LoadOrchestrator</outputDirectory>
							<artifactItems>
								<artifactItem>
									<groupId>com.ca.apm.systemtest.fld</groupId>
									<artifactId>dashboard-manager</artifactId>
									<classifier>dist</classifier>
									<type>zip</type>
								</artifactItem>
							</artifactItems>
						</configuration>
					</execution>
				</executions>
			</plugin>


			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<configuration>
					<url>${tomcat-url}</url>
					<path>/LoadOrchestrator</path>
					<!-- <update>true</update> -->
					<server>tomcat2</server>
				</configuration>
			</plugin>
		</plugins>
	</build>
