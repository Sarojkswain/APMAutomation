	</transformation>
	
	<files id="files">
		<file>${#emPlugin.pluginConfiguration.currentInstallDir}/Introscope_Enterprise_Manager.lax</file>
	</files>
	
	<binding id="binding">
		<transformation-ref id="props" />
		<files-ref id="files" /> 
	</binding>
</configuration>